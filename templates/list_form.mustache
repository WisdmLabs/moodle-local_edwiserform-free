<div class="efb-wrap-list-forms">
    <div class='efb-list-buttons text-right'>
        {{#pageactions}}
            <a class='btn btn-primary' href='{{url}}'>{{label}}</a>
        {{/pageactions}}
    </div>
    <hr>
    <h2>{{heading}}</h2>
    <table class="generaltable">
        <thead>
            <tr>
                {{#headings}}
                    <th>{{.}}</th>
                {{/headings}}
            </tr>
        </thead>
        <tbody>
            {{#rows}}
                <tr id="efb-form-id-{{id}}">
                    <td class="efb-tbl-col-title">{{title}}</td>
                    <td class="efb-tbl-col-type">{{type}}</td>
                    <td class="efb-tbl-col-shortcode">[edwiser-form id="{{id}}"]</td>
                    <td class="efb-tbl-col-author">{{author}}</td>
                    <td class="efb-tbl-col-create">{{created}}</td>
                    <td class="efb-tbl-col-modified">{{modified}}</td>
                    <td class="efb-tbl-col-action-list" data-formid="{{id}}" data-formtype="{{type}}">
                        {{#actions}}
                            <a {{#attrs}} {{key}}="{{{value}}}" {{/attrs}}>
                                <i class="{{icon}}" aria-hidden="true" title="{{title}}" aria-label="{{label}}"></i>
                            </a>
                        {{/actions}}
                    </td>
                </tr>
            {{/rows}}
            {{#norowsmsg}}
                <tr>
                    <td colspan="6">{{norowsmsg}}</td>
                </tr>
            {{/norowsmsg}}
        </tbody>
    </table>
</div>
