<div class="efb-forms-setup ">
    <div class="efb-forms-setup-title">
        {{panel_setup.heading}}
        <p class="efb-forms-setup-desc core">
            {{panel_setup.msg_select_tmpl}}
        </p>
    </div>

    <div id="efb-forms-setup-templates-core" class="efb-forms-setup-templates">
        <div class="efb-list">
            {{#panel_setup.list}}
                {{#pro}}
                    <div class="efb-forms-template" id="efb-forms-template-{{tmpl_id}}">
                        <div class="efb-forms-template-outer bg-primary">
                            <div class="efb-forms-template-inner">
                                <div class="efb-forms-template-name">
                                    {{tmpl_name}}
                                    <label class="efb-forms-pro-label bg-warning m-0">{{#str}} efb-pro-label, local_edwiserform {{/str}}</label>
                                </div>
                                <div class="efb-forms-template-details">
                                    <p class="desc">{{desc}}</p>
                                </div>
                                <div class="efb-forms-template-overlay">
                                    <a href="#" class="efb-forms-pro-select" data-template-name="{{tmpl_name}}" data-template="{{tmpl_id}}">
                                        {{panel_setup.btn_upgrade}}
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                {{/pro}}
                {{^pro}}
                    <div class="efb-forms-template {{#active}}active{{/active}}" id="efb-forms-template-{{tmpl_id}}">
                        <div class="efb-forms-template-outer bg-primary">
                            <div class="efb-forms-template-inner">
                                <div class="efb-forms-template-name">
                                    {{tmpl_name}}
                                    <label class="efb-forms-template-active bg-success m-0">{{#str}} active, local_edwiserform {{/str}}</label>
                                </div>
                                <div class="efb-forms-template-details">
                                    <p class="desc">{{desc}}</p>
                                </div>
                                <div class="efb-forms-template-overlay">
                                    <a href="#" class="efb-forms-template-select" {{#pro}}data-pro="true"{{/pro}} data-template-name="{{tmpl_name}}" data-template="{{tmpl_id}}">
                                        {{tmpl_hover_txt}}
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                {{/pro}}
            {{/panel_setup.list}}
        </div>
    </div>

    <div class="efb-forms-setup-title">
        {{panel_setup.msg_upgrade}}
        <a href="" target="_blank" rel="noopener noreferrer">
            {{panel_setup.btn_upgrade}}
        </a>
    </div>

</div>
